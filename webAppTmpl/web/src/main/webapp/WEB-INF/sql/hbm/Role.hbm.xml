<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="in.v8delta.domain.Role" table="ROLE">
		<meta attribute="class-description">
			This class is the Object Model for Roles
		</meta>
		<id name="roleId" type="int" column="ROLE_ID">
			<generator class="native" />
		</id>
		<property name="roleName" column="ROLE_NAME" type="string"/>
		<property name="roleDescription" column="ROLE_DESCRIPTION" type="string"/>
	</class>
</hibernate-mapping>
